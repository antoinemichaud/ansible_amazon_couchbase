{% for host in groups['tag_Name_couchbase_ansible_other_nodes'] %}
/opt/couchbase/bin/couchbase-cli server-add -c 127.0.0.1:8091 -u {{ couchbase_username }} \
-p {{ couchbase_password }} --server-add={{ hostvars[host]['ansible_eth0']['ipv4']['address'] }}:8091 \
--server-add-username={{ couchbase_username }} --server-add-password={{ couchbase_password }}
{% endfor %}
